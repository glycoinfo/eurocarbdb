# Synthesis and NMR spectroscopy of nine stereoisomeric 5,7-diacetamido-3,5,7,9-tetradeoxynon-2-ulosonic acids
# Y.E. Tsvetkov, A.S. Shashkov, Y.A. Knirel, U. ZÃ¤hringer
set ref 'Carbohydr. Res. 335 (2001) 221-243'
# These experiments are run at room temperature and at 30 deg C.

type '6dHex2,4NAc' {
info '2,4-diacetamido-2,4,6-trideoxyhexopyranoside'
priority '6p'
atom '1' 1 pyran link free
atom '2' 1 pyran
atom '3' 1 pyran free
atom '4' 1 pyran
atom '5' 1 pyran closing
atom '6' 1 methyl
atom 'M_2' 1 methyl
atom 'O_2' 0
atom 'M_4' 1 methyl
atom 'O_4' 0
connect '1' '2' '3' '4' '5' '6'
connect 'O_2' 'M_2'
connect 'O_4' 'M_4'
ctclass 'hex'
ctsub 'n-acetyl' 2 1 'd'
ctsub 'n-acetyl' 4 1 'd'
ctmod 'd' 6
ccpsub 'C2_NAc:C4_NAc'
}

# 25
# No shifts for NAc in the source. These are approximated. ML 080828
residue 'aD6dTal2,4NAc' '6dHex2,4NAc' {
config D
carbbank 'a-D-6-deoxy-TalpNAc4NAc'
ctname 'a-dtal'
jhh medium
shift 94.2 5.18
shift 52.8 4.12
shift 66.7 4.24
shift 52.7 4.18
shift 66.7 4.43
shift 17.0 1.18

shift 23 2.05
shift 175

shift 23 2.05
shift 175

info $ref
}
# 25
# No shifts for NAc in the source. These are approximated. ML 080828
residue 'bD6dTal2,4NAc' '6dHex2,4NAc' {
config D
carbbank 'b-D-6-deoxy-TalpNAc4NAc'
ctname 'b-dtal'
jhh medium
shift 94.6 4.97
shift 53.4 4.36
shift 69.6 4.12
shift 52.0 4.12
shift 71.7 3.91
shift 17.1 1.18

shift 23 2.05
shift 175

shift 23 2.05
shift 175

info $ref
}


type '9ddn5,7NAc' {
info '5,7-diacetamido-3,5,7,9-tetradeoxynon-2-ulosonic acid'
priority '9p'
atom '1' 0
atom '2' 0 link pyran free
atom '3' 2 pyran
atom '4' 1 pyran free
atom '5' 1 pyran
atom '6' 1 pyran closing
atom '7' 1
atom '8' 1 free
atom '9' 1 methyl
atom 'M_5' 1 methyl
atom 'O_5' 0
atom 'M_7' 1 methyl
atom 'O_7' 0
connect '1' '2' '3' '4' '5' '6' '7' '8' '9'
connect 'O_5' 'M_5'
connect 'O_7' 'M_7'
ctclass 'non'
ctsub 'n-acetyl' 5 1 'd'
ctsub 'n-acetyl' 7 1 'd'
ctmod 'a' 1
ctmod 'd' 3
ctmod 'd' 9
ccpsub 'C5_NAc:C7_NAc'
}

# 49a
# No shifts for NAc. Shifts from 49b used.
residue 'LGly-aDGal9ddn5,7NAc' '9ddn5,7NAc' {
