shift  77.31 3.95 #d
shift  80.09 #d
shift  74.32 3.91 4.06
shift  64.25 3.66 3.66
shift  56.78 3.46
}

# Check the structure of these!
residue 'threo-aDApiOMe' ApiOMe {
info 'Methyl 3-C-(hydroxymethyl)-b-L-threofuranoside'
