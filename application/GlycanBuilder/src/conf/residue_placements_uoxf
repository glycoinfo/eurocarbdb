% Regex				Positions	OnBorder	Sticky
pb & !cs &!cx			0		yes		yes
!ps & !pb & !pr			0		no		no
(!cs)&(!cx)&(lp=[1-3[N]])	-90		yes		yes
(!cs)&(!cx)&(lp=[4-9])		90		yes		yes
(!cs)&(!cx)			90,-90		yes		yes
lp=2				-90		no		no
lp=3				-45		no		no
lp=4				0		no		no
lp=6				45		no		no
lp=8				90		no		no
1				0		no		no