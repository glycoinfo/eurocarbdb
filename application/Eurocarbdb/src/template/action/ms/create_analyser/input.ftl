<form method="POST">
	Model: <input type="text" name="analyser.model"/><br/>
	Analyser Type: <input type="text" name="analyser.analyserType"/><br/>
	Resolution: <input type="text" name="analyser.resolution"/><br/>
	Accuracy: <input type="text" name="analyser.accuracy"/><br/>
	Scan Rate: <input type="text" name="analyser.scanRate"/><br/>
	Scan Direction: <input type="text" name="analyser.scanDirection"/><br/>
	Scan Law: <input type="text" name="analyser.scanLaw"/><br/>
	Reflector State: <input type="checkbox" value="true" name="analyser.reflectorState"/><br/>
	TOF path length: <input type="text" name="analyser.tofPathLength"/><br/>
	Isolation Width: <input type="text" name="analyser.isolationWidth"/><br/>
	Magnetic Field Strength: <input type="text" name="analyser.magneticFieldStrength"/><br/>
	<input type="submit"/>
</form>